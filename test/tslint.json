{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb",
        "tslint:recommended"
    ],
    "rules": {
        "semicolon": [true, "always"],
        "quotemark": [true, "single", "jsx-double"],
        "member-access": false,
        "object-literal-sort-keys": [true, "shorthand-first"]
    }
}