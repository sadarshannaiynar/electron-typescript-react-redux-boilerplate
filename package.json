{
  "name": "electron-typescript-react-redux-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for Electron Typescript, React and Redux",
  "main": "src/main/dist/main/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=prod gulp build",
    "build:main": "NODE_ENV=prod webpack --progress --config webpack.config.electron.js",
    "build:renderer": "NODE_ENV=prod webpack --progress --config webpack.config.renderer.js",
    "pregenerate": "rimraf build && npm run build",
    "generate": "electron-builder -mwl",
    "pregenerate:linux": "rimraf build && npm run build",
    "generate:linux": "npm run build && electron-builder --linux",
    "pregenerate:mac": "rimraf build && npm run build",
    "generate:mac": "npm run build && electron-builder --mac",
    "pregenerate:windows": "rimraf build && npm run build",
    "generate:windows": "npm run build && electron-builder --windows",
    "lint:renderer": "tslint -p ./src/renderer/tsconfig.json",
    "lint:main": "tslint -p ./src/main/tsconfig.json",
    "lint:styles": "stylelint \"./src/renderer/styles/**/*.scss\"",
    "lint:test": "tslint -p ./test/tsconfig.json",
    "lint": "npm run lint:renderer && npm run lint:main && npm run lint:styles && npm run lint:test",
    "prestart:main": "npm run build",
    "start:main": "electron ./src/main/dist/main/index.js",
    "start:main-dev": "cross-env NODE_ENV=dev gulp start:devwatch",
    "start:renderer-dev": "NODE_ENV=dev webpack-dev-server --config webpack.config.renderer.js",
    "pretest:renderer": "rimraf coverage/renderer",
    "test:renderer": "jest --notify --verbose -c jest.config.renderer.js",
    "test:renderer-watch": "jest --runInBand --watch --notify --verbose -c jest.config.renderer.js",
    "pretest:main": "rimraf coverage/main",
    "test:main": "jest --notify --verbose -c jest.config.main.js",
    "test:main-watch": "jest --runInBand --watch --notify --verbose -c jest.config.main.js",
    "pretest": "rimraf coverage",
    "test": "jest --notify --verbose",
    "test:watch": "jest --watch --notify --verbose",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "redux",
    "node"
  ],
  "license": "MIT",
  "dependencies": {
    "@types/enzyme": "^3.1.13",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.116",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-mock-store": "^1.0.0",
    "ajv": "^6.5.2",
    "autoprefixer": "^9.0.2",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "electron": "^2.0.6",
    "electron-builder": "^20.26.1",
    "electron-connect": "^0.6.3",
    "electron-ipc-mock": "^0.0.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "fancy-log": "^1.3.2",
    "file-loader": "^1.1.11",
    "growl": "^1.10.5",
    "gulp": "^4.0.0",
    "jest": "^23.5.0",
    "lodash": "^4.17.10",
    "mini-css-extract-plugin": "^0.4.1",
    "node-sass": "4.9.3",
    "plugin-error": "^1.0.1",
    "postcss": "^7.0.2",
    "postcss-loader": "^2.1.6",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-hot-loader": "^4.0.0",
    "react-redux": "^5.0.7",
    "react-test-renderer": "^16.4.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.3",
    "spawn-sync": "^2.0.0",
    "style-loader": "^0.21.0",
    "stylelint": "8.4.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.2.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-loader": "^3.6.0",
    "typesafe-actions": "^2.0.4",
    "typescript": "^3.0.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "snyk": "^1.258.2"
  },
  "snyk": true
}
